{
  "name": "@vostro/parton",
  "version": "0.1.1",
  "description": "parton",
  "module": "lib/index.js",
  "main": "lib/cjs/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/Azerothian/parton",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "demo": "./cli.sh --cwd ./demo/",
    "apps:server": "./cli.sh --cwd ./apps/server/",
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s test:*",
    "test:lint": "eslint src --ext .ts",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "nyc --silent jest",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "nyc --silent jest --watch",
    "cov": "run-s build test:unit cov:html cov:lcov && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:lcov": "nyc report --reporter=lcov",
    "cov:send": "run-s cov:lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && yarn",
    "prepare-release": "run-s reset-hard test cov:check doc:html version doc:publish",
    "prepublishOnly": "yarn build && yalc retreat --all",
    "postpublish": "yalc restore"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "dependencies": {
    "@graphql-tools/stitch": "^9.0.4",
    "@vostro/gqlize": "^5.3.3",
    "@vostro/gqlize-adapter-sequelize": "^5.3.0",
    "@vostro/graphql-jtd": "^1.0.4",
    "@vostro/graphql-types": "^2.0.0",
    "@vostro/object-visit": "^1.0.1",
    "@vostro/sandwich": "^0.2.0",
    "bcrypt": "^5.1.1",
    "debug": "^4.3.4",
    "deepmerge": "^4.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "glob": "^10.3.10",
    "graphql": "^16.8.1",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^5.1.1",
    "ioredis": "^5.3.2",
    "is-plain-obj": "^4.1.0",
    "minimatch": "^9.0.3",
    "minimist": "^1.2.8",
    "object-hash": "^3.0.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "prom-client": "^15.1.0",
    "sequelize": "^6.37.1",
    "umzug": "^3.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "20",
    "@types/object-hash": "^3.0.6",
    "@types/passport": "^1.0.16",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vostro/jtd-types": "^1.0.2",
    "codecov": "^3.8.2",
    "copyfiles": "^2.4.1",
    "cspell": "^8.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.29.1",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "jest-mock-req-res": "^1.0.2",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^8.0.0",
    "pg": "^8.11.3",
    "prettier": "^3.2.5",
    "sqlite3": "^5.1.7",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3",
    "v8-profiler-next": "^1.10.0"
  },
  "files": [
    "lib/**/*",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
