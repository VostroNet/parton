{
  "name": "@parton/core",
  "version": "0.1.2",
  "description": "parton",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/Azerothian/parton",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "prepare": "husky || true",
    "demo": "./cli.sh --cwd ./demo/",
    "apps:server": "./cli.sh --cwd ./apps/server/",
    "build": "rm -Rf ./lib ./types ./cjs || true; run-p build:*",
    "build:main": "swc src --out-dir lib --strip-leading-paths -s",
    "build:types": "tsc -p tsconfig.types.json",
    "build:copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" lib",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s test:*",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "nyc --silent jest",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "nyc --silent jest --watch",
    "cov": "run-s build test:unit cov:html cov:lcov && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:lcov": "nyc report --reporter=lcov",
    "cov:send": "run-s cov:lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && yarn",
    "prepare-release": "run-s reset-hard test cov:check doc:html version doc:publish",
    "prepublishOnly": "pnpm run build; echo next; yalc retreat --all",
    "postpublish": "yalc restore --all && pnpm install"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "dependencies": {
    "@azerothian/object-loader": "^0.0.3",
    "@graphql-tools/stitch": "^9.2.10",
    "@types/cors": "^2.8.17",
    "@types/finalhandler": "^1.2.3",
    "@vostro/gqlize": "file:.yalc/@vostro/gqlize",
    "@vostro/gqlize-adapter-sequelize": "file:.yalc/@vostro/gqlize-adapter-sequelize",
    "@vostro/graphql-jtd": "^1.0.4",
    "@vostro/graphql-types": "^2.0.0",
    "@vostro/object-visit": "^1.1.0",
    "@vostro/sandwich": "^0.3.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "debug": "^4.3.6",
    "deepmerge": "^4.3.1",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "finalhandler": "^1.2.0",
    "glob": "^11.0.0",
    "globby": "^14.0.2",
    "graphql": "^16.9.0",
    "graphql-relay": "^0.10.2",
    "graphql-scalars": "^1.23.0",
    "graphql-yoga": "^5.7.0",
    "ioredis": "^5.4.1",
    "is-plain-obj": "^4.1.0",
    "jose": "^5.7.0",
    "minimatch": "^10.0.1",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "object-hash": "^3.0.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "sequelize": "^6.37.3",
    "umzug": "^3.8.1",
    "uuid": "^10.0.0",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jest-mock/express": "^2.1.0",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.14",
    "@swc/plugin-transform-imports": "^2.0.12",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "22",
    "@types/node-schedule": "^2.1.7",
    "@types/object-hash": "^3.0.6",
    "@types/passport": "^1.0.16",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "@vostro/jtd-types": "^1.0.2",
    "codecov": "^3.8.2",
    "copyfiles": "^2.4.1",
    "cspell": "^8.14.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "jest-mock-req-res": "^1.0.2",
    "mkdirp": "^3.0.1",
    "npm-run-all2": "^6.2.2",
    "nyc": "^17.0.0",
    "open-cli": "^8.0.0",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "sqlite3": "^5.1.7",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.17.0",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4",
    "v8-profiler-next": "^1.10.0"
  },
  "files": [
    "lib/**/*",
    "src/**/*",
    "!**/*.test.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
